\ProvidesPackage{beamerthememonbafclean}[2021/12/06]

\mode<presentation>



% Load the subthemes
\useinnertheme{monbafclean}
\useoutertheme{monbafclean}
\usecolortheme{monbafclean}

% Font package
\usepackage[scaled]{helvet} % text font
\usepackage{newtxmath} % math font

% Fonts
\setbeamerfont{title}{series=\mdseries}
\setbeamerfont{subtitle}{series=\mdseries}
\setbeamerfont{author}{size=\small}
\setbeamerfont{date}{size=\footnotesize}
\setbeamerfont{institute}{size=\footnotesize}
\setbeamerfont{frametitle}{size=\Large}
\setbeamerfont{framesubtitle}{size=\normalsize}
\setbeamerfont{pagenumber}{size=\tiny,series=\bfseries}


\setbeamercolor{titlelike}{fg=monashblue,bg=white}

% Turn off navigation symbols
\setbeamertemplate{navigation symbols}{}

\mode<all>
